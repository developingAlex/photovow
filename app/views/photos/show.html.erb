<p id="notice"><%= notice %></p>

<p>
  <strong>&lt;Image data&gt;</strong>
  <%= @photo.image_data %>
</p>

<p>
  <%= "by #{@photo.portfolio.user.first_name}" %>
</p>

<p>
  <%= @photo.description %>
</p>
<% if @photo.portfolio.user == current_user %>
  <%= link_to 'Edit', edit_photo_path(@photo) %> | 
  <%= link_to 'Delete', @photo, method: :delete %> | 
<% end %>
<%= link_to 'Back', :back %>
  
