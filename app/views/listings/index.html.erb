<div class="search align-self-center ">
  <%= form_tag(@listing, method: "get") do %>
    <%= text_field_tag :search, params[:search], placeholder: "Search listings" %>
    <%= submit_tag "Search" %>
  <% end %>
</div>

<% @listings.each do |listing| %>
  <div class="listing">
    <h3>
      <%= link_to listing.title, listing %>
    </h3>
  </div>
<% end %>

